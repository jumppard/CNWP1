@using TestWebApp.ViewModel
@*@model CardTransferViewModel*@

@using PagedList.Mvc
@model  PagedList.IPagedList<TestWebApp.ViewModel.CardTransferViewModel>

@{
    ViewBag.Title = "Card Transfer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Card Transfer</h2>

@Html.Partial("_CardTransferTable", Model)

<div id="container" style="margin-left: 0px">
    <p></p>
    <p></p>
    <div class="pagination" style="margin-left: 0px">
        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber)
        of @Model.PageCount   @Html.PagedListPager(Model, page => Url.Action("Index", new { page }))
    </div>
</div>