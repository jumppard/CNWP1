@using TestWebApp.ViewModel
@*@model CardTransferListViewModel*@

@using PagedList.Mvc
@model  PagedList.IPagedList<TestWebApp.ViewModel.CardTransferViewModel>

<div>
    <table>
        <tr class="tableHeader">
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Cardnet ID            </th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Request ID            </th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Customer Request ID      </th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Customer              </th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Transfer Type           </th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Transfer Case       </th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Balance Transfer     </th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Bonus Transfer              </th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Cardno From Final Status</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Account Type Final Status</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Cardno From</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Cardno To</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Account Type</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Inserted</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Last Update</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Request Status</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Successfull</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">Is Processing</th>
            <th style="padding: 1.0rem;border: solid thin;background-color:lightgray;text-align:center">SLA Deadline</th>


        </tr>

        @if (Model.Any())
            {
                foreach (var item in Model)
                {
                <tr bgcolor=@item.RowColor>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.ID</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.RequestId            </th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.PartnerRequestId      </th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.PartnerName</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.TransferType         </th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.TransferCase       </th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.BalanceTransfer   </th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.BonusTransfer</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.CardnoFromFinalStatus</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.AccountTypefinalStatus</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.CardnoFrom</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.CardnoTo</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.AccountTypeIdName</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.CreationDateTime</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.LastUpdateDate</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.RequestStatus</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.Succesfull</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.IsProcessing</th>
                    <th style="padding: 1.0rem;border: solid thin;text-align:center; border-color: rgba(0, 0, 0, 0.3)">@item.SlaDeadline</th>
                </tr>
            }
        }
        else
        {
            <tr>
                <td>empty</td>
            </tr>
        }
    </table>
</div>




