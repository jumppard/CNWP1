@using TestWebApp.ViewModel
@*@model CardStatusUpdateListViewModel*@

@using PagedList.Mvc
@model  PagedList.IPagedList<TestWebApp.ViewModel.CardStatusUpdateViewModel>


@{
    ViewBag.Title = "Card Status Update";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Card Status Update</h2>

@Html.Partial("_CardStatusUpdateTable",Model)    

<div id="container" style="margin-left: 0px">
    <p></p>
    <p></p>
    <div class="pagination" style="margin-left: 0px">
        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber)
        of @Model.PageCount   @Html.PagedListPager(Model, page => Url.Action("Index", new { page }))
    </div>
</div>

